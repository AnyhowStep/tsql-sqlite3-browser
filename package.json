    "@types/bluebird": "^3.5.30",
    "@types/node": "^13.9.0",
    "@types/tape": "^4.2.34",
    "bluebird": "^3.7.2",
    "tape": "^4.13.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "type-mapping": "^1.32.0"
  }
}
